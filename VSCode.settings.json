{
    "workbench.colorCustomizations": {
        "[Default Dark+]": {
            "editorCursor.background": "#f00",
            // "editorCodeLens.foreground": "#ff0000"
            "editor.background": "#3d3c3c",
            // "editor.foreground": "#ff0000",
            "editorCursor.foreground": "#fff"
        }
    },
    "git.path": "D:/zhanghao/systemtools/Git/bin/git.exe",
    "terminal.integrated.shell.windows": "D:\\zhanghao\\systemtools\\Git\\bin\\bash.exe",
    "window.zoomLevel": 1,
    "editor.fontSize": 15,
    "editor.minimap.enabled": false,
    "files.autoSave": "afterDelay",
    "editor.renderIndentGuides": true,
    "editor.renderLineHighlight": "all",
    "editor.showFoldingControls": "always",
    "files.autoGuessEncoding": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.copyOnSelection": true,
    "editor.wordWrapColumn": 180,
    "prettier.bracketSpacing": false,
    "prettier.printWidth": 300,
    "prettier.tabWidth": 4,
    // 强制单引号
    "prettier.singleQuote": true,
    // 尽可能控制尾随逗号的打印
    "prettier.trailingComma": "all",
    // 开启 eslint 支持
    "prettier.eslintIntegration": false,
    "editor.tabSize": 4,
    // "eslint.options": {
    //     "configFile": "D:/codes/cloudpipelineui_angular7_dev/.eslintrc.js"
    // },
    ////////////////////////////
    "color-highlight.matchWords": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorWidth": 2,
    "editor.detectIndentation": false,
    "editor.find.autoFindInSelection": true,
    "editor.fontFamily": "YaHei Consolas Hybrid, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.lineHeight": 24,
    "editor.mouseWheelZoom": true,
    "editor.quickSuggestions": {
        "comments": true,
        "other": true,
        "strings": true
    },
    "editor.rulers": [],
    "editor.snippetSuggestions": "top",
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.tabCompletion": "on",
    "editor.wordWrap": "on",
    "emmet.syntaxProfiles": {
        "vue": "html",
        "vue-html": "html"
    },
    "emmet.triggerExpansionOnTab": true,
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        {
            "autoFix": true,
            "language": "vue"
        }
    ],
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.associations": {
        "*.vue": "vue",
        "*.wxss": "css"
    },
    "files.autoSaveDelay": 1000,
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.history": true,
        "**/.idea": true,
        "**/.settings": true,
        "**/.svn": true,
        "**/.vscode": true,
        "**/CVS": true,
        "**/node_modules": true,
        "**/tmp": true,
        "**/yarn.lock": true,
        "*.war": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.enableSmartCommit": true,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.keymap": "alternate",
    "http.proxyAuthorization": null,
    "http.proxyStrictSSL": false,
    "npm.enableScriptExplorer": true,
    "search.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.gitignore": true,
        "**/.idea": true,
        "**/.svn": true,
        "**/.vscode": false,
        "**/build": true,
        "**/dist": true,
        "**/elehukouben": true,
        "**/tmp": true,
        "**/yarn.lock": true
    },
    "search.smartCase": true,
    "symbolOutline.doSort": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "tslint.alwaysShowRuleFailuresAsWarnings": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "update.showReleaseNotes": false,
    "vetur.format.defaultFormatterOptions": {
        "prettier": {
            "semi": true,
            "singleQuote": true
        },
        "prettyhtml": {
            "printWidth": 120,
            "singleQuote": false,
            "sortAttributes": true,
            "wrapAttributes": false
        }
    },
    "vetur.format.options.tabSize": 4,
    "vetur.validation.template": false,
    "vsicons.dontShowNewVersionMessage": true,
    "vsicons.projectDetection.autoReload": true,
    "window.restoreWindows": "all",
    "workbench.commandPalette.preserveInput": true,
    "workbench.startupEditor": "none",
    "workbench.view.alwaysShowHeaderActions": true,
    "eslint.enable": false,
    "leetcode.endpoint": "leetcode-cn",
    "http.proxy": "http://proxy.huawei.com:8080",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[less]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "vscode.markdown-language-features"
    },
}
